<?xml version="1.0" encoding="utf-8"?>
<KeyboardEx
    ns:keyWidth="@fraction/default_10_key_width"
    ns:canSwypePopupCharacters="false"
    xmlns:ns="http://schemas.android.com/apk/res-auto">
    <include
        ns:src="@xml/kbd_qwerty_sym_row_default" />
    <Row
        ns:rowEdgeFlags="top"
        ns:rowBackground="?firstRowBackground">
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_q"
            ns:keyEdgeFlags="left"
            ns:keyLabel="1"
            ns:shiftedKeyLabel="`"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_w"
            ns:keyLabel="2"
            ns:shiftedKeyLabel="™"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwertyeuro_panlatin_e"
            ns:keyLabel="3"
            ns:shiftedKeyLabel="®"
            ns:keyGroup="@id/currency1"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_e"
            ns:keyLabel="3"
            ns:shiftedKeyLabel="€"
            ns:keyboardSettings="dollar"
            ns:keyGroup="@id/currency1"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwertypound_panlatin_e"
            ns:keyLabel="3"
            ns:shiftedKeyLabel="€"
            ns:keyboardSettings="pound"
            ns:keyGroup="@id/currency1"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_r"
            ns:keyLabel="4"
            ns:shiftedKeyLabel="₹"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_t"
            ns:keyLabel="5"
            ns:shiftedKeyLabel="^"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_y"
            ns:keyLabel="6"
            ns:shiftedKeyLabel="¥"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_u"
            ns:keyLabel="7"
            ns:shiftedKeyLabel="«"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_i"
            ns:keyLabel="8"
            ns:shiftedKeyLabel="»"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_o"
            ns:keyLabel="9"
            ns:shiftedKeyLabel="{"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_p"
            ns:keyEdgeFlags="right"
            ns:keyLabel="0"
            ns:shiftedKeyLabel="}"
            ns:keyBackground="@drawable/btn_keyboard_key_top"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
    </Row>
    <Row
        ns:rowBackground="?secondRowBackground">
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_a"
            ns:keyEdgeFlags="left"
            ns:keyLabel="\@"
            ns:shiftedKeyLabel="©"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwertyeuro_panlatin_s"
            ns:keyLabel="€"
            ns:shiftedKeyLabel="$"
            ns:keyGroup="@id/currency2"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_s"
            ns:keyLabel="$"
            ns:shiftedKeyLabel="®"
            ns:keyboardSettings="dollar"
            ns:keyGroup="@id/currency2"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwertypound_panlatin_s"
            ns:keyLabel="£"
            ns:shiftedKeyLabel="$"
            ns:keyboardSettings="pound"
            ns:keyGroup="@id/currency2"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_d"
            ns:keyLabel="&amp;"
            ns:shiftedKeyLabel="|"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_f"
            ns:keyLabel="_"
            ns:shiftedKeyLabel="°"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_g"
            ns:keyLabel="("
            ns:shiftedKeyLabel="["
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_h"
            ns:keyLabel=")"
            ns:shiftedKeyLabel="]"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_j"
            ns:keyLabel=":"
            ns:shiftedKeyLabel="&lt;"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_k"
            ns:keyLabel=";"
            ns:shiftedKeyLabel=">"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwertyeuro_panlatin_l"
            ns:keyEdgeFlags="right"
            ns:keyLabel="&quot;"
            ns:shiftedKeyLabel="£"
            ns:keyGroup="@id/currency3"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_l"
            ns:keyEdgeFlags="right"
            ns:keyLabel="&quot;"
            ns:shiftedKeyLabel="£"
            ns:keyboardSettings="dollar"
            ns:keyGroup="@id/currency3"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwertypound_panlatin_l"
            ns:keyEdgeFlags="right"
            ns:keyLabel="&quot;"
            ns:shiftedKeyLabel="®"
            ns:keyboardSettings="pound"
            ns:keyGroup="@id/currency3"
            ns:resizeWhenHidden="false"
            ns:keyBackground="@drawable/btn_keyboard_key_mid"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
    </Row>
    <Row
        ns:rowBackground="?thirdRowBackground">
        <Key
            ns:keyWidth="@fraction/shift_7_10_key_width"
            ns:codes="@integer/keycode_shift"
            ns:keyEdgeFlags="left"
            ns:isModifier="true"
            ns:isSticky="true"
            ns:iconPreview="@drawable/sym_keyboard_sym_toggle_feedback"
            ns:keyIcon="@drawable/sym_keyboard_sym_toggle"
            ns:keyIconRecolor="?iconRecolorSymbolsToggle"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_z"
            ns:keyLabel="*"
            ns:shiftedKeyLabel="¢"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_x"
            ns:keyLabel="!"
            ns:shiftedKeyLabel="¡"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_c"
            ns:keyLabel="\#"
            ns:shiftedKeyLabel="%"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_v"
            ns:keyLabel="="
            ns:shiftedKeyLabel="¤"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_b"
            ns:keyLabel="/"
            ns:shiftedKeyLabel="\\"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_n"
            ns:keyLabel="+"
            ns:shiftedKeyLabel="~"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:popupCharacters="@string/alternates_for_qwerty_panlatin_m"
            ns:keyLabel="\?"
            ns:shiftedKeyLabel="¿"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
        <Key
            ns:keyWidth="@fraction/delete_7_10_key_width"
            ns:codes="@integer/keycode_delete"
            ns:keyEdgeFlags="right"
            ns:isRepeatable="false"
            ns:iconPreview="?symKeyboardFeedbackDelete"
            ns:keyIcon="?symKeyboardDelete"
            ns:keyIconRecolor="?iconRecolorDelete"
            ns:altPreviewIcon="?symKeyboardFeedbackDelete"
            ns:keyType="function"
            ns:keyBackground="@drawable/btn_keyboard_action_key"
            ns:keyContentBaseline="@dimen/key_content_no_baseline" />
    </Row>
    <include
            ns:src="@xml/kbd_bottom_row_default_sym" />
</KeyboardEx>